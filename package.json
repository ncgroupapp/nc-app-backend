{
  "name": "nestjs-service-template",
  "version": "1.0.0",
  "description": "Template for new nestjs services",
  "author": "alberthernandezdev@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlbertHernandez/nestjs-service-template/issues"
  },
  "homepage": "https://github.com/AlbertHernandez/nestjs-service-template#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbertHernandez/nestjs-service-template.git"
  },
  "keywords": [
    "typescript",
    "nestjs",
    "template"
  ],
  "engines": {
    "node": ">=22.x",
    "pnpm": ">=9.x"
  },
  "packageManager": "pnpm@9.14.2",
  "main": "dist/main.js",
  "scripts": {
    "build": "node --run build:clean && nest build --path tsconfig.prod.json",
    "start": "node dist/main.js",
    "dev": "nest start --watch --debug=0.0.0.0:9229",
    "test": "rimraf coverage .nyc_output && concurrently 'node --run test:unit' 'node --run test:e2e' && node --run calculate-global-test-coverage",
    "test:unit": "vitest run --coverage --config vitest.config.unit.ts",
    "test:e2e": "vitest run --coverage --config ./vitest.config.e2e.ts",
    "test:performance": "k6 run tests/performance/contexts/users/get-users.mjs",
    "calculate-global-test-coverage": "tsx scripts/calculate-global-test-coverage.ts",
    "prepare": "[ -f .husky/install.mjs ] && node .husky/install.mjs || true",
    "lint": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts",
    "lint:fix": "eslint --ignore-path .gitignore . --ext .js,.mjs,cjs,.ts,.mts --fix",
    "lint:file": "eslint --ignore-path .gitignore",
    "lint:yaml": "chmod +x scripts/lint_yaml.sh && ./scripts/lint_yaml.sh",
    "build:clean": "rimraf dist; exit 0",
    "typos": "chmod +x scripts/check_typos.sh && ./scripts/check_typos.sh",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "m:gen": "npm run typeorm -- migration:generate -d src/config/typeorm.ts src/migrations/UpdateDB",
    "m:run": "npm run typeorm -- migration:run -d src/config/typeorm.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^7.0.4",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/common": "^10.4.16",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.6",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^10.4.6",
    "@nestjs/swagger": "^8.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/pdfkit": "^0.17.3",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "fastify": "^4.28.1",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.8.1",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.13.5",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.8.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-istanbul": "1.3.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-vitest": "^0.4.1",
    "fs-extra": "^11.3.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nock": "^13.5.5",
    "nyc": "^17.1.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "unplugin-swc": "^1.5.5",
    "vite": "^5.4.21",
    "vitest": "^1.6.1",
    "vitest-mock-extended": "^1.3.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.24.3",
    "@swc/core-linux-arm64-musl": "^1.13.21",
    "@swc/core-linux-x64-gnu": "^1.13.3",
    "@swc/core-linux-x64-musl": "^1.13.3"
  }
}
